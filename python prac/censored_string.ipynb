    "def uncensored(txt,vowels):\n",
    "    l1=[]\n",
    "    counter=0\n",
    "    for i in txt:\n",
    "        l1.append(i)\n",
    "    for j in l1:\n",
    "        if j=='*':\n",
    "            index_j=l1.index(j)\n",
    "            l1[index_j]=vowels[counter]\n",
    "            counter+=1\n",
    "    return \"\".join(l1)\n",
    "uncensored('*m*an*','aeo')\n"
   ]
